{
  "extends": "../../packages/config/tsconfig/nextjs.json",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "plugins": [{ "name": "next" }]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../../types/**/*.d.ts"
  ],
  "exclude": ["node_modules", ".next", ".open-next"]
}
